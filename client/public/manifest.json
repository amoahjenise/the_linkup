{
  "short_name": "Linkup",
  "name": "The Linkup",
  "id": "thelinkup-pwa",
  "description": "Connect with new people and organize meetups around your interests.",
  "icons": [
    {
      "src": "/logo-no-bg-white.png",
      "sizes": "180x180",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/web-app-manifest-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "start_url": "/?source=pwa&homescreen=1",
  "scope": "/",
  "badge": "/badge-icon.png",
  "display_override": ["standalone", "minimal-ui", "window-controls-overlay"],
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#000000",
  "orientation": "portrait-primary",
  "categories": ["social", "lifestyle", "events"],
  "shortcuts": [
    {
      "name": "My Linkups",
      "short_name": "My Linkups",
      "description": "A list of my created linkups.",
      "url": "/history",
      "icons": [{ "src": "/favicon-96x96.png", "sizes": "96x96" }]
    }
  ],
  "lang": "en",
  "related_applications": [
    {
      "id": "thelinkup-pwa",
      "platform": "webapp",
      "url": "/manifest.json"
    }
  ],
  "prefer_related_applications": true,
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+thelinkup",
      "url": "/?url=%s"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/homepage.png",
      "description": "Linkup homepage on mobile",
      "sizes": "372x812",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/screenshots/homepage-wide.png",
      "description": "Linkup homepage on desktop",
      "sizes": "1918x927",
      "type": "image/png",
      "form_factor": "wide"
    }
  ]
}
