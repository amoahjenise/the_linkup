{
  "short_name": "The Linkup",
  "name": "The Linkup",
  "id": "/",
  "description": "Connect with new people and organize meetups around your interests.",
  "icons": [
    {
      "src": "/badge-icon.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "badge"
    },
    {
      "src": "/web-app-manifest-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/web-app-manifest-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/apple-touch-icon.png",
      "sizes": "180x180",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "start_url": "/?source=pwa&homescreen=1",
  "scope": "/",
  "display_override": ["standalone", "minimal-ui"],
  "display": "standalone",
  "badge": "/badge-icon.png",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "orientation": "portrait-primary",
  "categories": ["social", "lifestyle", "events"],
  "shortcuts": [],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+thelinkup",
      "url": "/?url=%s"
    }
  ],
  "screenshots": []
}
